{
  "name": "jsreport-authorization",
  "version": "1.1.1",
  "description": "jsreport extension adding authorization cappabilities",
  "scripts": {
    "start": "jsreport-studio-start --httpPort 5488",
    "run": "npm run build && jsreport-studio-start --httpPort 5488 --run-only",
    "test": "mocha test --timeout 5000 && standard",
    "build": "jsreport-studio-build",
    "prepublish": "in-publish && jsreport-studio-build || not-in-publish"
  },
  "homepage": "https://github.com/jsreport/jsreport-authorization",
  "repository": {
    "type": "git",
    "url": "git@github.com:jsreport/jsreport-authorization.git"
  },
  "keywords": [
    "jsreport",
    "authorization"
  ],
  "standard": {
    "ignore": [
      "main.js"
    ],
    "env": {
      "node": true,
      "mocha": true,
      "browser": true
    },
    "parser": "babel-eslint"
  },
  "license": "LGPL",
  "dependencies": {
    "bluebird": "3.5.1"
  },
  "author": "Jan Blaha",
  "devDependencies": {
    "babel-eslint": "8.2.1",
    "eslint": "4.17.0",
    "eslint-config-standard-jsx": "4.0.2",
    "eslint-plugin-babel": "4.1.2",
    "in-publish": "2.0.0",
    "jsreport-core": "github:jsreport/jsreport-core",
    "jsreport-studio-dev": "1.1.2",
    "jsreport-templates": "github:jsreport/jsreport-templates",
    "mocha": "5.0.0",
    "should": "13.2.1",
    "standard": "10.0.3"
  },
  "files": [
    "lib",
    "studio",
    "index.js",
    "jsreport.config.js"
  ],
  "engines": {
    "node": ">=8.9"
  }
}
