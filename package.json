{
  "name": "jsreport-authorization",
  "version": "1.0.2",
  "description": "jsreport extension adding authorization cappabilities",
  "scripts": {
    "test": "mocha test --timeout 5000 && standard",
    "build": "npm run prepublish",
    "prepublish": "in-publish && jsreport-studio-build || not-in-publish"
  },
  "homepage": "https://github.com/jsreport/jsreport-authorization",
  "repository": {
    "type": "git",
    "url": "git@github.com:jsreport/jsreport-authorization.git"
  },
  "keywords": [
    "jsreport",
    "authorization"
  ],
  "standard": {
    "ignore": [
      "main.js"
    ],
    "env": {
      "node": true,
      "mocha": true,
      "browser": true
    },
    "parser": "babel-eslint"
  },
  "license": "LGPL",
  "dependencies": {
    "bluebird": "3.4.7",
    "in-publish": "2.0.0",
    "node.extend": "1.1.6",    
    "request": "2.79.0",
    "underscore": "1.8.3"
  },
  "author": "Jan Blaha",
  "devDependencies": {
    "babel-eslint": "7.1.1",
    "eslint": "3.14.1",
    "eslint-config-standard-jsx": "3.2.0",
    "eslint-plugin-babel": "4.0.1",
    "jsreport-core": "1.1.0",
    "jsreport-studio-dev": "1.0.2",
    "jsreport-templates": "1.1.0",
    "mocha": "3.2.0",
    "should": "11.2.0",
    "standard": "8.6.0"
  },
  "files": [
    "lib",
    "studio",
    "index.js",
    "jsreport.config.js"
  ]
}
