{
  "name": "jsreport-authorization",
  "version": "0.3.0",
  "description": "jsreport extension adding authorization cappabilities",
  "scripts": {
    "test": "mocha test --timeout 5000 && standard",
    "build": "npm run prepublish",
    "prepublish": "in-publish && jsreport-studio-build || not-in-publish"
  },
  "homepage": "https://github.com/jsreport/jsreport-authorization",
  "repository": {
    "type": "git",
    "url": "git@github.com:jsreport/jsreport-authorization.git"
  },
  "keywords": [
    "jsreport",
    "authorization"
  ],
  "standard": {
    "ignore": [
      "main.js"
    ],
    "env": {
      "node": true,
      "mocha": true,
      "browser": true
    },
    "parser": "babel-eslint"
  },
  "license": "LGPL",
  "dependencies": {
    "in-publish": "2.0.0",
    "node.extend": "1.1.5",
    "q": "1.4.1",
    "request": "2.72.0",
    "underscore": "1.8.3"
  },
  "author": "Jan Blaha",
  "devDependencies": {
    "babel-eslint": "6.0.4",
    "eslint": "2.11.1",
    "eslint-config-standard-jsx": "1.2.1",
    "eslint-plugin-babel": "3.2.0",
    "jsreport-core": "*",
    "jsreport-studio-dev": "0.2.8",
    "jsreport-templates": "*",
    "mocha": "2.5.3",
    "should": "9.0.1",
    "standard": "7.1.2"
  }
}
