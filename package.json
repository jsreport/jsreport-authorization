{
  "name": "jsreport-authorization",
  "version": "2.2.6",
  "description": "jsreport extension adding authorization cappabilities",
  "scripts": {
    "start": "jsreport-studio-start --httpPort 5488",
    "run": "npm run build && jsreport-studio-start --httpPort 5488 --run-only",
    "test": "mocha test --timeout 5000 && standard",
    "build": "jsreport-studio-build",
    "prepublish": "in-publish && jsreport-studio-build || not-in-publish"
  },
  "homepage": "https://github.com/jsreport/jsreport-authorization",
  "repository": {
    "type": "git",
    "url": "git@github.com:jsreport/jsreport-authorization.git"
  },
  "keywords": [
    "jsreport",
    "authorization"
  ],
  "standard": {
    "ignore": [
      "main.js",
      "test/data"
    ],
    "env": {
      "node": true,
      "mocha": true,
      "browser": true
    },
    "parser": "babel-eslint"
  },
  "license": "LGPL",
  "dependencies": {
    "node.extend.without.arrays": "1.1.6"
  },
  "author": "Jan Blaha",
  "devDependencies": {
    "babel-eslint": "10.0.1",
    "eslint": "5.10.0",
    "eslint-config-standard-jsx": "6.0.2",
    "eslint-plugin-babel": "5.3.0",
    "in-publish": "2.0.1",
    "jsreport-core": "2.8.1",
    "jsreport-fs-store": "2.8.0",
    "jsreport-studio-dev": "1.5.0",
    "jsreport-templates": "2.3.3",
    "mocha": "5.2.0",
    "ncp": "2.0.0",
    "rimraf": "2.6.3",
    "should": "13.2.3",
    "standard": "12.0.1"
  },
  "files": [
    "lib",
    "studio",
    "index.js",
    "jsreport.config.js"
  ],
  "engines": {
    "node": ">=8.9"
  }
}
